version: '2'

services:
  tomcat8:
    build: .
    image: tomcat-#BUILDTAG
    container_name: tomcat-#BUILDTAG
    ports:
      - "8080"
    environment:
      - DB_HOST=db-#BUILDTAG
    depends_on:
      - mysql5
    

  mysql5:
    image: mysql:5.7
    container_name: db-#BUILDTAG
    environment:
      - MYSQL_ROOT_PASSWORD=root
      - MYSQL_DATABASE=booking

